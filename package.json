{
  "name": "material-kit-react",
  "version": "1.3.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn build-css && react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build-package": "npm run build-package-css && babel src --out-dir dist",
    "build-package-css": "node-sass-chokidar src/assets/scss/material-kit-react.scss dist/material-kit-react.css",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "lint": "yarn lint:check",
    "lint:check": "eslint src --ext=js,jsx",
    "lint:fix": "eslint src --ext=js,jsx --fix",
    "prepare": "yarn build",
    "start": "node index.js",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "CI=true react-scripts test --env=jsdom",
    "watch": "npm-run-all -p watch-css start-js",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "dependencies": {
    "@material-ui/core": "3.1.1",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@material-ui/system": "^3.0.0-alpha.2",
    "classnames": "2.2.6",
    "express": "^4.16.4",
    "history": "4.7.2",
    "lodash.assign": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "moment": "2.22.2",
    "nouislider": "12.0.0",
    "npm-run-all": "4.1.3",
    "prop-types": "15.6.2",
    "react": "^16.8.6",
    "react-bash": "^1.6.0",
    "react-datetime": "2.15.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.5",
    "react-slick": "0.23.1",
    "react-swipeable-views": "0.13.0",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-import-rename": "1.0.1",
    "babel-plugin-module-resolver": "3.1.1",
    "eslint-plugin-import": "^2.14.0",
    "node-sass-chokidar": "^1.3.5"
  },
  "optionalDependencies": {
    "ajv": "5.0.0",
    "babel-eslint": "7.2.3",
    "eslint": "4.19.1",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "prettier": "1.14.3"
  }
}
